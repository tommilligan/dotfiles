{
    "editor.renderWhitespace": "all",
    "editor.tabSize": 2,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j","j"],
            "after": ["<Esc>"]
        }
    ],
    "explorer.confirmDragAndDrop": false,
    "flow.useNPMPackagedFlow": true,
    "javascript.validate.enable": false,
    "eslint.enable": true,
    "gitlens.keymap": "alternate"
}